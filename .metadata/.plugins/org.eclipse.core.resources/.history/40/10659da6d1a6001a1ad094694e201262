package com.bit.day23;

import java.awt.Dialog;
import java.awt.Frame;

public class Ex03 extends Frame {

	public Ex03() {
		
		setSize(800, 600);
		setLocation(200, 200);
		setVisible(true);
		
		java.awt.Dialog dia = new Dialog(this);
		dia.setSize(200,100);
		dia.setLocation(200+800/2, 200+600/2);
		dia.setVisible(true);
	
	
	}
	
	public static void main(String[] args) {
		new Ex03();
	}
}
